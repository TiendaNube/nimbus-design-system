{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build:styles": {
      "dependsOn": ["^build:styles"]
    },
    "build:essentials": {
      "dependsOn": ["^build:styles", "^build:essentials"]
    },
    "build": {
      "dependsOn": ["^build:essentials", "^build"]
    }
  }
}
